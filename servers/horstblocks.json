{
  "name": "HorstBlocks",
  "address": [
    "horstblocks.de",
    "horstblocks.net"
  ],
  "rooms": [
    {
      "type": "private",
      "format": [
        {
          "direction": [
            "INBOUND"
          ],
          "meta": {
            "sender": "{Sender}",
            "message": "{Message}"
          },
          "match": [
            [
              "[",
              "{Sender}",
              " -> ",
              "Du",
              "] ",
              "{Message}"
            ]
          ]
        },
        {
          "direction": [
            "OUTBOUND"
          ],
          "meta": {
            "receiver": "{Receiver}",
            "message": "{Message}"
          },
          "match": [
            [
              "[",
              "Du -> ",
              "{Receiver}",
              "] ",
              "{Message}"
            ]
          ]
        }
      ]
    }
  ]
}