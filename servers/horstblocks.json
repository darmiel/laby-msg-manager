{
  "name": "HorstBlocks",
  "address": [
    "horstblocks.de",
    "horstblocks.net"
  ],
  "rooms": [
    {
      "type": "private",
      "format": [
        {
          "direction": [
            "INBOUND"
          ],
          "match": [
            [
              "[",
              "{{ Sender }}",
              " -> ",
              "Du",
              "] ",
              "{{ Message }}"
            ]
          ]
        },
        {
          "direction": [
            "OUTBOUND"
          ],
          "match": [
            [
              "[",
              "Du -> ",
              "{{ Receiver }}",
              "] ",
              "{{ Message }}"
            ]
          ]
        }
      ]
    }
  ]
}