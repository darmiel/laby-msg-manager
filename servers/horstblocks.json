{
  "name": "HorstBlocks",
  "rooms": [
    {
      "type": "private",
      "format": [
        {
          "direction": [
            "IN"
          ],
          "meta": {
            "sender": "{Sender}",
            "message": "{Message}"
          },
          "match": [
            [
              "[",
              "{Sender}",
              " -> ",
              "Du",
              "] ",
              "{Message}"
            ]
          ]
        },
        {
          "direction": [
            "OUT"
          ],
          "meta": {
            "receiver": "{Receiver}",
            "message": "{Message}"
          },
          "match": [
            [
              "[",
              "Du -> ",
              "{Receiver}",
              "] ",
              "{Message}"
            ]
          ]
        }
      ]
    }
  ]
}